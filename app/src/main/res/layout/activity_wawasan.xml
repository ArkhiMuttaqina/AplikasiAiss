<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@drawable/bgwawasan"
    tools:context=".WawasanActivity">

    <RelativeLayout
        android:layout_width="350dp"
        android:layout_height="670dp"
        android:layout_marginStart="5dp"
        android:layout_marginTop="50dp">

        <ImageButton
            android:id="@+id/menuAlat"
            android:layout_width="350dp"
            android:layout_height="160dp"
            android:layout_marginTop="14dp"
            android:layout_marginEnd="1dp"
            android:background="@drawable/menualat"
            android:scaleType="fitXY"
            android:contentDescription="@string/todo" />
        <ImageButton
            android:id="@+id/menuTari"
            android:layout_width="350dp"
            android:layout_height="160dp"
            android:layout_marginTop="172dp"
            android:layout_marginEnd="1dp"
            android:background="@drawable/menutari"
            android:scaleType="fitXY"
            android:contentDescription="@string/todo" />

        <ImageButton
            android:id="@+id/menuSuku"
            android:layout_width="350dp"
            android:layout_height="160dp"
            android:layout_marginTop="330dp"
            android:layout_marginEnd="1dp"
            android:background="@drawable/menusuku"
            android:contentDescription="@string/todo"
            android:scaleType="fitXY" />

        <ImageButton
            android:id="@+id/menuRumah"
            android:layout_width="350dp"
            android:layout_height="160dp"
            android:layout_marginTop="490dp"
            android:layout_marginEnd="1dp"
            android:background="@drawable/menurumah"
            android:contentDescription="@string/todo"
            android:scaleType="fitXY" />
    </RelativeLayout>




</RelativeLayout>