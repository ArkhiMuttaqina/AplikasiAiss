<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".rumah">

    <ImageView
        android:layout_width="720dp"
        android:layout_height="1280dp"
        android:background="@drawable/bgadat"
        android:scaleType="centerCrop"
        android:contentDescription="@string/todo" />

    <RelativeLayout
        android:layout_width="356dp"
        android:layout_height="460dp"
        android:layout_marginStart="5dp"
        android:layout_marginTop="250dp"
        >

        <ImageButton
            android:id="@+id/waleng"
            android:layout_width="176dp"
            android:layout_height="233dp"
            android:hapticFeedbackEnabled="true"
            android:background="@drawable/wawangko"
            android:contentDescription="@string/todo"
 />
        <ImageButton
            android:id="@+id/kebaya"
            android:layout_width="176dp"
            android:layout_height="233dp"
            android:hapticFeedbackEnabled="true"
            android:layout_marginStart="174dp"
            android:background="@drawable/kabaya"
            android:contentDescription="@string/todo" />
        <ImageButton
            android:id="@+id/gadang"
            android:layout_width="176dp"
            android:layout_height="233dp"
            android:hapticFeedbackEnabled="true"
            android:layout_marginTop="230dp"
            android:background="@drawable/gadang"
            android:contentDescription="@string/todo" />
        <ImageButton
            android:id="@+id/salaso"
            android:layout_width="176dp"
            android:layout_height="233dp"
            android:hapticFeedbackEnabled="true"
            android:layout_marginTop="230dp"
            android:layout_marginStart="174dp"
            android:background="@drawable/salaso"
            android:contentDescription="@string/todo" />
    </RelativeLayout>


</RelativeLayout>